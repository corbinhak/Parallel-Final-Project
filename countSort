#include<stdio.h>
#include<stdlib.h>
 
void counting_sort(int list[],int n){
    //creates an array of all the possible inputs
    int match[15+1]={0};
    

    //compares and calculates the final array
    for(int i = 0; i < n; i++){
        for(int j = 0; j <= n; j++){
            if(list[i] == j){
                match[j] = match[j] + 1;
            }
        }
    }

    //prints list
    printf("Output Array = [");
    for(int i = 0; i <= n; i++){
        while(match[i]>0){
            if(i != n){
                printf("%d,", i);
            }
            else{
                printf("%d", i);
            }
            match[i]--;
        }
    }
    printf("]\n");
}
 
int main(){
    int n = 15;
    int *list = (int*)malloc(n * sizeof(int));

    printf("Input Array = [");
    for(int i = 0; i <= n; i++){
        list[i] = rand() % 16;
        if(i != n){
            printf("%d,", list[i]);
        }
        else{
            printf("%d", list[i]);
        }
    }
    printf("]\n");

    counting_sort(list,n);
    return 0;
}
